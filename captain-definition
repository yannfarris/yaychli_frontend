{
  "schemaVersion" :2 ,
  "dockerfileLines": [
   "FROM node:14.5.0-alpine AS build",
   "WORKDIR /app/",
   "COPY package*.json /app/",
   "RUN npm ci",
   "COPY . /app/",
   "RUN npm run build",
   "FROM nginx:1.17.1-alpine",
   "COPY nginx.conf /etc/nginx/nginx.conf",
   "COPY --from=build /app/_static /usr/share/nginx/html",
   "EXPOSE 80"
 ]
 }
 
 
 